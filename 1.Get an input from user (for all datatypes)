
//1.Get an input from user (for all datatypes int,str,char)
import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc=new Scanner(System.in);
	    
	    byte by=sc.nextByte();
		System.out.println(by);
		
		short sh=sc.nextShort();
		System.out.println(sh);
		
	    int num= sc.nextInt();
		System.out.println(num);
		
		float dec= sc.nextFloat();
		System.out.println(dec);
		
		long l=sc.nextLong();
		System.out.println(l);
		
		double point= sc.nextDouble();
		System.out.println(point);
		
		String str= sc.next();
		System.out.println(str);
		sc.nextLine();//this code used, only we get the string next line in the middle .
		//if we use them in the beginning there is no need to use it .
		
		String str1= sc.nextLine();
		System.out.println(str1);
		
		char alphabet= sc.next().charAt(2);
		System.out.println(alphabet);
	}
}

OUTPUT:
30
30
10
10
999
999
20.6
20.6
100
100
20.668
20.668
sushmitaa
sushmitaa
hello guys 
