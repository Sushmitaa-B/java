import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc= new Scanner (System.in);
      int num= sc.nextInt();
      int count=0;
      for(int i=2;i<=num/2;i++){
        count++;
      }
      count=count+2;
      if(count==2){
        System.out.println("yes");
      }
      else{
        System.out.println("no");
      }
    }
}


//hakkerrank
import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);
        
        // Read input for the cost and quantity of three items, plus the total budget
        int cost1 = sc.nextInt();
        int quantity1 = sc.nextInt();
        int cost2 = sc.nextInt();
        int quantity2 = sc.nextInt();
        int cost3 = sc.nextInt();
        int quantity3 = sc.nextInt();
        int budget = sc.nextInt();
        
        // Calculate the total cost before tax
        float b_tax = ((cost1 * quantity1) + (cost2 * quantity2) + (cost3 * quantity3));
        
        // Calculate the total cost after applying a 10% tax
        double a_tax = b_tax + (0.10 * b_tax);
        
        // Print the total cost before tax
        System.out.printf("Total (Before Tax): %.2f%n", b_tax);
        
        // Print the total cost after tax, formatted to two decimal places
        System.out.printf("Total (After Tax): %.2f%n", a_tax);
        
        // Check if the after-tax total is within or over the budget
        if (a_tax <= budget) {
            System.out.println("Within Budget");
        } else {
            System.out.println("Over Budget");
        }
        
        sc.close(); // Close the scanner to prevent resource leaks
    }
}





