// 4.To check whether a number is a palindrome or not
import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner sc= new Scanner(System.in);
		int a=sc.nextInt();
		int min=1;
		while(a!=0) {
			int r=a%10;
			if(min>r){
			    min=r;
			}
			
			a=a/10;

		}
		System.out.println(min);
	

	}
}


