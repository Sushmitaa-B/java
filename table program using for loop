import java.util.Scanner;
public class Main
{
	public static void main(String[] args) {
	    Scanner no = new Scanner(System.in);
        int i;
        System.out.println("Enter the number to print multiplication table ");
        int j=no.nextInt();
        
        
        
        for (i=1;i<=10;i++){
            
                System.out.println(  i +"*"+ j + "=" + i*j);
        }
		
	}
}
