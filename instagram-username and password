


import java.util.Scanner;
public class Main
{
	public static String [] setuserpass(){
	   
	   
	    Scanner x = new Scanner(System.in);
        String[] arr = new String[2];
        
	    System.out.println("Enter user name to set:");
	    arr[0]=x.nextLine();
		
 	
	    System.out.println("Enter password to set:");
	    arr[1]=x.nextLine();
	    
	    return arr;
	    
	}
		public static String [] getUserPass(){
	   
	   
	    Scanner y = new Scanner(System.in);
        String[] array = new String[2];
        
	    System.out.println("Enter user name:");
	    array[0]=y.nextLine();
		
 	
	    System.out.println("Enter password:");
	    array[1]=y.nextLine();
	    
	    return array;
	    
	}
	
	public static void main(String[] args) {

        String [] arr = setuserpass();
        String [] array=getUserPass();
        if (arr[0].equals(array[0]) && arr[1].equals(array[1])){
            System.out.println("LOGGED IN SUCCESSFULLY!");
        }
        else{
            System.out.println("EITHER USER NAME OR PASSWORD WRONG");
        }
	}
}




Enter user name to set:
admin
Enter password to set:
admin@123
Enter user name:
admin
Enter password:
admin@123
LOGGED IN SUCCESSFULLY!
