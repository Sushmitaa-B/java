1.//maximum odd and even in an array
  
  import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      int n=sc.nextInt();
      int arr []=new int[n];
      for(int i=0;i<n;i++){
        arr[i]+=sc.nextInt();
      }
      int even=Integer.MIN_VALUE;
      int odd=Integer.MIN_VALUE;
      for(int i=0;i<n;i++){
        if(arr[i]%2==0){
          if(arr[i]>even){
            even=arr[i];
          }
        }
        else{
          if(arr[i]>odd){
            odd=arr[i];
          }
        }
      }System.out.println("Maximum Even: "+even);
      System.out.println("Maximum Odd: "+odd);
    }    
}


7
5 8 11 24 19 30 2
Output:
Maximum Even: 30
Maximum Odd: 19

2.//Methods------->set of code used when it is called only(code reusability)
//prime No--->a no which has only 2 factors
//with arg with return type

import java.util.*;

public class Main {
  
  
  
    public static boolean prime(int a){
      int count=0;
      for(int i=1;i<=a;i++){
        if(a%i==0){
          count++;
        }
      }
      if(count==2){
        return true;
      }
      else{
        return false;
      }
      
    }
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      int num=sc.nextInt();
      int num1=sc.nextInt();
      boolean f=prime(num);//store the result in a variable and print it 
      System.out.println(f);
      System.out.println(prime(num1));//directly print the return value
      
      
    }
    
}

7
20
Output:
true
false

3.//
//with arg without return type

import java.util.*;

public class Main {
  
  
  
    public static void prime(int a){
      int count=0;
      for(int i=1;i<=a;i++){
        if(a%i==0){
          count++;
        }
      }
      if(count==2){
        System.out.println(true); //here we directly print the return 
        // in the method rather than in main method
      }
      else{
        System.out.println( false);
      }
      
    }
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      int num=sc.nextInt();
      int num1=sc.nextInt();
      prime(num);
      prime(num1);
      
    }
    
}

11
15
Output:
true
false

4.
//with arg without return type(non static)

import java.util.*;

public class Main {
  
  
  
    public void prime(int a){//here we 
      int count=0;
      for(int i=1;i<=a;i++){
        if(a%i==0){
          count++;
        }
      }
      if(count==2){
        System.out.println(true); //here we directly print the return 
        // in the method rather than in main method
      }
      else{
        System.out.println( false);
      }
      
    }
    public static void main(String[] args) {
      Scanner sc=new Scanner(System.in);
      int num=sc.nextInt();
      int num1=sc.nextInt();
      Main m=new Main();//here we created a obj for the main class to access the prime method 
      m.prime(num);
      m.prime(num1);
      
    }
    
}

21
66
Output:
false
false

5.////without arg with return type
import java.util.*;
public class Main{
    public static boolean prime(){
        Scanner sc = new Scanner(System.in);
        int a=sc.nextInt();
        int count=0;
        for(int i=1;i<=a;i++){
            if(a%i==0){
                count++;
            }
        }
        if(count==2){
            return true;
        }
        else{
          return false;
        }
    }
	public static void main(String[] args) {
	   System.out.println(prime());
	   System.out.println(prime());
	  
	}
}
5
true
3
