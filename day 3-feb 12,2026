5.Even Odd Split

import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n= sc.nextInt();
        int arr[]= new int[n];
        for(int i=0;i<n;i++){
            arr[i]=sc.nextInt();
            if(arr[i]%2==0){
            System.out.print(arr[i]+" ");
            
            }
            
        }
        System.out.println();
        for (int i=0;i<n;i++){
            //if (arr[i]%2==1 || arr[i]%2==-1)
             if(arr[i]%2!=0){
                System.out.print(arr[i]+" ");
            }
       
           
        }
        
        
    }
}


