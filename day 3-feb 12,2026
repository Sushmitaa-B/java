13.Find the Second Maximum Element in an Array
import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int size = sc.nextInt();
        int arr[] = new int[size];
        for(int i = 0; i < size; i++){
            arr[i] = sc.nextInt();
        }
        int max = arr[0];
        int smax = arr[1];
        if(max < smax){
            int temp = max;
            max = smax;
            smax = temp;
        }
        for(int i = 2; i < size; i++){
            if(arr[i] > max){
                smax = max;
                max = arr[i];
            }
            else if(arr[i] > smax && arr[i] != max){
                smax = arr[i];
            }
        }

        System.out.println(smax);
    }
}


