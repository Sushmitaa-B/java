6.average of an element
import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n= sc.nextInt();
        int arr[]= new int[n];
        double sum=0;
        for(int i=0;i<n;i++){
            arr[i]=sc.nextInt();
            sum+=arr[i];
            } 
        System.out.printf("%.2f",sum/n);
        }
        
        
        
        
    
        
        
    }

