10.Alternate Index Product
import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        int n=sc.nextInt();
        
        int arr []=new int [n];
        for(int i=0;i<n;i++){
           arr[i]=sc.nextInt();
        }
        int eproduct=1;
        int oproduct=1;
        for(int i=0;i<n;i++){
            if(i%2==0){
                eproduct*=arr[i];
            }
            else{
                oproduct*=arr[i];
            }
        }
        System.out.println(eproduct);
        System.out.println(oproduct);
        
        
    }
}

