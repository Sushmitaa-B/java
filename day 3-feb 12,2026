14.Find Leader Elements in an Array
import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        int n=sc.nextInt();
        int arr[]=new int [n];
        for(int i=0;i<n;i++){
            arr[i]=sc.nextInt();
        }
        int max=Integer.MIN_VALUE;
        for(int i=0;i<n;i++){
          int f=0;
            for(int j=i+1;j<n;j++){
               if(arr[i]<arr[j]){
                 f=1;
                 break;
               }
            }
            if(f==0){
            System.out.print(arr[i]+" ");
            }
        }
        
    }
}

