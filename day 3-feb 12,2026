15.Find Mid element of an Array
import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc= new Scanner(System.in);
        int n=sc.nextInt();
        int arr[]=new int [n];
        for(int i=0;i<n;i++){
            arr[i]=sc.nextInt();
        }
       
        if(n%2!=0){
            System.out.println(arr[n/2]);
        }
        
        else{
            System.out.println((arr[(n/2)-1]+arr[n/2])/2);
        }
                
    }
}

