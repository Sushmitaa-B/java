public class Main
{
	public static void main(String[] args) {
	    //hashCode()
	    String n="Sushmitaa baskaran";
	    System.out.println(n.hashCode());
// 		char y= 'H';
// 		System.out.println(y.hashCode());
		
		//toUpperCase() and toLowerCase()
		String name="Sushmitaa baskaran";
		System.out.println(name.toUpperCase());
		System.out.println(name.toLowerCase());
		
		//indexOf()
		System.out.println(name.indexOf("baskaran"));
		String r="hi,my name is sushmitaa and i am from pudukkottai,i am pursuing my degree B.Tech in Artificial Intelligence and Data Science";
		System.out.println(r.indexOf("pudukkottai"));
		
		//isEmpty()
		String s1="Hello World";
		String s2="";
		System.out.println(s1.isEmpty());
		System.out.println(s2.isEmpty());
		
		
	}
}

output:
118233652
SUSHMITAA BASKARAN
sushmitaa baskaran
10
38
false
true


public class Main
{
	public static void main(String[] args) {
	    //join()
	    String name=String.join(" ","Sushmitaa","Tarshita","Ashwin");
	    System.out.println(name);
	    
		
		
	}
}
output:
Sushmitaa Tarshita Ashwin


public class Main
{
	public static void main(String[] args) {
	    //lastIndexOf()
	    String name="Sushmitaa baskaran";
	    System.out.println(name.lastIndexOf("baskaran"));
	    
	    //length
	     System.out.println(name.length());
		
		
	}
}
