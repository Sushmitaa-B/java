public class Main {
    public static void main(String[] args) {
        int num = 145;
        int original = num;
        int i = 2;

        while (num % i == 0) {
            num = num / i;
            i++;
        }

        if (num == 1) {
            System.out.println(original + " is a factorial of " + (i - 1) + "!");
        } else {
            System.out.println(original + " is NOT a factorial of any integer.");
        }
    }
}
