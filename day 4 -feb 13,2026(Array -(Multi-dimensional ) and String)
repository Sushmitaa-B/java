1.//2D array 
import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc= new Scanner(System.in);
      // int arr[][]={{1,2,3},{4,5,6},{7,8,9}};//compile time
      int rsize=sc.nextInt();
      int csize=sc.nextInt();
      int arr[][]=new int [rsize][csize];
      
      for(int i=0;i<rsize;i++){
        for(int j=0;j<csize;j++){
          arr[i][j]=sc.nextInt();
          
        }
      }
      
      
      for(int i=0;i<rsize;i++){
        for(int j=0;j<csize;j++){
          System.out.print(arr[i][j]+" ");
          
        }
        System.out.println();
      }
      
    }
}


3
3
1 2 3 
 4 5 6
7 8 9
Output:
1 2 3 
4 5 6 
7 8 9 

2.//sum of all elements in a 2D array
  
import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc= new Scanner(System.in);
      // int arr[][]={{1,2,3},{4,5,6},{7,8,9}};//compile time
      int rsize=sc.nextInt();
      int csize=sc.nextInt();
      int arr[][]=new int [rsize][csize];
      
      for(int i=0;i<rsize;i++){
        for(int j=0;j<csize;j++){
          arr[i][j]=sc.nextInt();
          
        }
      }
      
      int sum=0;
      for(int i=0;i<rsize;i++){
        for(int j=0;j<csize;j++){        
          sum+=arr[i][j];          
         }
      }
      System.out.print(sum);
      
    }
}

3
3
1 2 3 
 4 5 6
7 8 9
Output:
45 

3.// sum of all corner elements in a 2D array (border elements)
import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc= new Scanner(System.in);
      // int arr[][]={{1,2,3},{4,5,6},{7,8,9}};//compile time
      int rsize=sc.nextInt();
      int csize=sc.nextInt();
      int arr[][]=new int [rsize][csize];
      
      for(int i=0;i<rsize;i++){
        for(int j=0;j<csize;j++){
          arr[i][j]=sc.nextInt();
          
        }
      }
      
      int sum=0;
      for(int i=0;i<rsize;i++){
        for(int j=0;j<csize;j++){
          //i==0 means 0 th row ,j==0 means last row and i==rsize-1 means last row 
          if(i==0 || j==0 || i==rsize-1|| j==csize-1){
            sum+=arr[i][j];
           }          
        }}
      System.out.print(sum);
      }
}

3
3
1 2 3 
 4 5 6
7 8 9
Output:
40 

4.//sumof all diagonal elements (left diagonal) in a 2D array 
import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc= new Scanner(System.in);
      // int arr[][]={{1,2,3},{4,5,6},{7,8,9}};//compile time
      int rsize=sc.nextInt();
      int csize=sc.nextInt();
      int arr[][]=new int [rsize][csize];
      
      for(int i=0;i<rsize;i++){
        for(int j=0;j<csize;j++){
          arr[i][j]=sc.nextInt();
          
        }
      }
      
      int sum=0;
      for(int i=0;i<rsize;i++){
        for(int j=0;j<csize;j++){
          
          if(i==j){
            sum+=arr[i][j];
            
          }
          
        }}
      System.out.print(sum+" ");
      
    }
}

3
3
1 2 3 
 4 5 6
7 8 9
Output:
15 
5.//sumof all diagonal (left and right ) elements  diagonal in a 2D array 
import java.util.*;

public class Main {
    public static void main(String[] args) {
      Scanner sc= new Scanner(System.in);
      // int arr[][]={{1,2,3},{4,5,6},{7,8,9}};//compile time
      int rsize=sc.nextInt();
      int csize=sc.nextInt();
      int arr[][]=new int [rsize][csize];
      
      for(int i=0;i<rsize;i++){
        for(int j=0;j<csize;j++){
          arr[i][j]=sc.nextInt();
          
        }
      }
      
      int sum=0;
      for(int i=0;i<rsize;i++){
        for(int j=0;j<csize;j++){
          
          if(i==j|| i+j==rsize-1){
            sum+=arr[i][j];
            
          }
          
        }}
      System.out.print(sum+" ");
      
    }
}

3
3
1 2 3 
 4 5 6
7 8 9
Output:
25 

